{
  "name": "@nx-starter/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve starter-api",
    "start:api": "nx serve starter-api",
    "start:web": "nx serve starter-pwa",
    "start:all": "nx run-many -t serve -p starter-api --parallel & nx run-many -t dev -p starter-pwa --parallel",
    "dev": "nx dev starter-pwa",
    "dev:api": "nx serve starter-api",
    "dev:web": "nx dev starter-pwa",
    "dev:all": "nx run-many -t serve -p starter-api --parallel & nx run-many -t dev -p starter-pwa --parallel",
    "build": "nx run-many -t build -p starter-api,starter-pwa",
    "build:api": "nx build starter-api",
    "build:web": "nx build starter-pwa",
    "build:libs": "nx run-many -t build -p domain-core,application-core,utils-core",
    "build:all": "nx run-many -t build --all",
    "build:prod": "nx run-many -t build -p starter-api,starter-pwa --configuration=production",
    "test": "nx run-many -t test --all",
    "test:api": "nx test starter-api",
    "test:web": "nx test starter-pwa",
    "test:libs": "nx run-many -t test -p domain-core,application-core,utils-core",
    "test:watch": "nx run-many -t test --all --watch",
    "test:coverage": "nx run-many -t test --all --coverage",
    "test:affected": "nx affected -t test",
    "e2e": "nx run-many -t e2e -p starter-api-e2e,starter-pwa-e2e",
    "e2e:api": "nx e2e starter-api-e2e",
    "e2e:web": "nx e2e starter-pwa-e2e",
    "e2e:headed": "nx run-many -t e2e -p starter-api-e2e,starter-pwa-e2e --headed",
    "lint": "nx run-many -t lint --all",
    "lint:api": "nx lint starter-api",
    "lint:web": "nx lint starter-pwa",
    "lint:libs": "nx run-many -t lint -p domain-core,application-core,utils-core",
    "lint:fix": "nx run-many -t lint --all --fix",
    "lint:affected": "nx affected -t lint",
    "typecheck": "nx run-many -t typecheck --all",
    "typecheck:api": "nx typecheck starter-api",
    "typecheck:web": "nx typecheck starter-pwa",
    "typecheck:libs": "nx run-many -t typecheck -p domain-core,application-core,utils-core",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "nx reset && rm -rf dist tmp",
    "clean:cache": "nx reset",
    "clean:all": "nx reset && rm -rf dist tmp node_modules && npm install",
    "projects": "nx show projects",
    "deps:graph": "nx dep-graph --view",
    "affected": "nx show projects --affected || echo 'No main branch found'",
    "ci": "npm run lint && npm run typecheck && npm run test && npm run build",
    "ci:affected": "nx affected -t lint,typecheck,test,build --parallel",
    "prepare:prod": "npm run clean && npm install && npm run ci && npm run build:prod",
    "setup": "npm install && npm run build:libs",
    "setup:dev": "npm install && npm run build:libs && npm run dev:all"
  },
  "private": true,
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.12",
    "@tailwindcss/vite": "^4.1.11",
    "axios": "^1.6.0",
    "better-sqlite3": "^11.10.0",
    "body-parser": "^2.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dexie": "^4.0.11",
    "dotenv": "^17.2.0",
    "empty": "0.10.1",
    "express": "^4.21.2",
    "immer": "^10.1.1",
    "lucide-react": "^0.525.0",
    "mongoose": "^8.16.4",
    "mysql2": "^3.14.2",
    "pg": "^8.16.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.60.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "tailwind-merge": "^3.3.1",
    "tsyringe": "^4.10.0",
    "tw-animate-css": "^1.3.5",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0",
    "zod": "^4.0.5",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/devkit": "21.2.3",
    "@nx/eslint": "21.2.3",
    "@nx/eslint-plugin": "21.2.3",
    "@nx/express": "^21.2.3",
    "@nx/jest": "21.2.3",
    "@nx/js": "^21.2.3",
    "@nx/node": "^21.2.3",
    "@nx/playwright": "21.2.3",
    "@nx/react": "^21.2.3",
    "@nx/vite": "^21.2.3",
    "@nx/web": "21.2.3",
    "@nx/webpack": "21.2.3",
    "@nx/workspace": "21.2.3",
    "@playwright/test": "^1.36.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "24.0.14",
    "@types/pg": "^8.15.4",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.4",
    "jest-environment-node": "^29.7.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.2.3",
    "postcss": "^8.5.6",
    "prettier": "^2.6.2",
    "react-refresh": "^0.10.0",
    "supertest": "^7.1.3",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vitest": "^3.2.4",
    "webpack-cli": "^5.1.4"
  }
}
